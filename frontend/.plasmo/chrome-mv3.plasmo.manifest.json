{
  "icons": {
    "16": "./gen-assets/icon16.plasmo.png",
    "32": "./gen-assets/icon32.plasmo.png",
    "48": "./gen-assets/icon48.plasmo.png",
    "64": "./gen-assets/icon64.plasmo.png",
    "128": "./gen-assets/icon128.plasmo.png"
  },
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "./gen-assets/icon16.plasmo.png",
      "32": "./gen-assets/icon32.plasmo.png",
      "48": "./gen-assets/icon48.plasmo.png",
      "64": "./gen-assets/icon64.plasmo.png",
      "128": "./gen-assets/icon128.plasmo.png"
    },
    "default_popup": "./popup.html"
  },
  "version": "0.0.1",
  "author": "Gabriel Moterani <hi@gabeismy.name>",
  "name": "DEV | Vix extension",
  "description": "Web Accessibility Enhancer",
  "background": {
    "service_worker": "./static/background/index.ts"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "static/content.tsx"
      ]
    }
  ],
  "host_permissions": [
    "https://*/*"
  ]
}
